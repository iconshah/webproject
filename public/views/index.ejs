  <script>
    // Function to check the number of cards and show/hide the "Show More" button
    function checkCardCount() {
      var cardContainer = document.getElementById("cardContainer");
      var cards = cardContainer.querySelectorAll(".card");

      // Only show the first three cards
      for (var i = 0; i < cards.length; i++) {
        if (i < 3) {
          cards[i].classList.remove("d-none"); // Show the card
        } else {
          cards[i].classList.add("d-none"); // Hide the card
        }
      }

      // Show or hide the "Show More" button based on the number of cards
      var showMoreBtn = document.getElementById("showMoreBtn");
      if (cards.length >= 4) {
        showMoreBtn.classList.remove("d-none"); // Show the button
      } else {
        showMoreBtn.classList.add("d-none"); // Hide the button
      }
    }

    // Check card count when the page loads
    window.addEventListener("load", checkCardCount);

    // Redirect to a new page when "Show More" button is clicked
    document
      .getElementById("showMoreBtn")
      .addEventListener("click", function () {
        window.location.href = "new_page.html";
      });
  </script>

